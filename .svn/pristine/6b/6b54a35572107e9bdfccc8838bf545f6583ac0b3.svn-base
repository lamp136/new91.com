@charset "UTF-8";
/*浏览器样式重置*/
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed,
figure, figcaption, footer, header, hgroup,
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
  margin: 0;
  padding: 0;
  border: 0;
  font: inherit;
  font-size: 100%;
  vertical-align: baseline;
}

html {
  line-height: 1;
}

ol, ul {
  list-style: none;
}

table {
  border-collapse: collapse;
  border-spacing: 0;
}

caption, th, td {
  text-align: left;
  font-weight: normal;
  vertical-align: middle;
}

q, blockquote {
  quotes: none;
}
q:before, q:after, blockquote:before, blockquote:after {
  content: "";
  content: none;
}

a img {
  border: none;
}

article, aside, details, figcaption, figure, footer, header, hgroup, main, menu, nav, section, summary {
  display: block;
}

a {
  color: #333333;
}

a, a:hover {
  text-decoration: none;
}

.clear {
  clear: both;
}

.clearfix {
  overflow: hidden;
  *zoom: 1;
}

body, p, h1, h2, h3, h4, h5, h6 {
  font-family: "宋体";
  font-size: 14px;
  color: #333333;
}

.contain {
  width: 1110px;
  margin: 0 auto;
}

.index-sprite, .navbar_top .nav_top li i, .navbar_top .nav_top .scan .toolbar-layer, .header .search .btn, .city .sele .sele_bg, .city_list .ci_title .c_close {
  /*background-image: url('../../images/zhongyuanjie/index-s21f9eef049.png');*/
  background-repeat: no-repeat;
}
/*顶部*/
.navbar_top {
  line-height: 34px;
  background: #fafafa;
  color: #999999;
  font-size: 12px;
  border-bottom: 1px solid #eeeeee;
}
.navbar_top .greet {
  float: left;
}
.navbar_top .greet span {
  margin-right: 68px;
}
.navbar_top .greet a {
  margin-right: 14px;
  color: #999999;
}
.navbar_top .greet .login {
  color: #ff6d00;
}
.navbar_top .nav_top {
  float: right;
}
.navbar_top .nav_top li {
  margin-right: 24px;
  float: left;
}
.navbar_top .nav_top li a {
  color: #999999;
}
.navbar_top .nav_top li i {
  background-position: 0 -14px;
  height: 4px;
  width: 7px;
  display: inline-block;
  margin-left: 3px;
}
.navbar_top .nav_top li img {
  vertical-align: middle;
  margin-bottom: 2px;
}
.navbar_top .nav_top .last {
  margin-right: 0;
}
.navbar_top .nav_top .scan {
  position: relative;
  cursor: pointer;
}
.navbar_top .nav_top .scan:hover .toolbar-layer {
  filter: progid:DXImageTransform.Microsoft.Alpha(enabled=false);
  opacity: 1;
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  -webkit-transform: scale(1);
  transform: scale(1);
}
.navbar_top .nav_top .scan .toolbar-layer {
  display: inline-block;
  background-position: 0 -54px;
  height: 168px;
  width: 129px;
  -moz-transform-origin: 98% 0;
  -ms-transform-origin: 98% 0;
  -webkit-transform-origin: 98% 0;
  transform-origin: 98% 0;
  position: absolute;
  filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=0);
  opacity: 0;
  -moz-transform: scale(0.01);
  -ms-transform: scale(0.01);
  -webkit-transform: scale(0.01);
  transform: scale(0.01);
  -moz-transition: all 1s;
  -o-transition: all 1s;
  -webkit-transition: all 1s;
  transition: all 1s;
  z-index: 999;
  right: 0;
  top: 35px;
}

/*header*/
.header {
  height: 84px;
}
.header .logo {
  padding-top: 8px;
  float: left;
}
.header .logo img {
  margin: 6px 10px 0 0;
}
.header h1 {
  float: left;
  font-size: 18px;
  font-family: "微软雅黑";
  line-height: 46px;
  border-left: 1px solid #eeeeee;
  padding-left: 20px;
  margin: 20px 0 0 10px;
}

.header .search .tex {
  width: 202px;
  float: left;
  height: 28px;
  line-height: 28px\9;
  border: 1px solid #fbb784;
  -moz-border-radius: 5px 0 0 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px 0 0 5px;
}
.header .search .btn {
  float: left;
  border: none;
  background-position: 0 -20px;
  height: 32px;
  width: 44px;
}
.header .header_phone {
  float: right;
  padding-top: 30px;
}
.header .contain {
  position: relative;
}

/*专题导航*/
.topics_nav {
  background: #f7f7f7;
  width: 100%;
  height: 40px;
  z-index: 99999;
  top: 0;
}
.nav-con{
  width: 1200px;
  margin:0 auto;
}
.topics_nav ul li {
  float: left;
  font-size: 16px;
  font-weight: bold;
}
.topics_nav ul li a {
  display: block;
  width: 170px;
  line-height: 40px;
  color: #333333;
  text-align: center;
}
.topics_nav .currter a {
  background: #41b98d;
  color: #fff;
  width: 130px;
}

/*底部样式*/
.topics_footer {
  line-height: 70px;
  text-align: center;
  font-family: "微软雅黑";
}