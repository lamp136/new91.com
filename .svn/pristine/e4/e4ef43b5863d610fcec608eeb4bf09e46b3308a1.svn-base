    var _handleImages = function(data, k) {
        var x = '',
            cla = '';
        if (k % 3 == 0) {
            cla = 'class="last"';
        }
        x += '<li ' + cla + '>';
        x += '<a class="fancybox" href="' + data['image'] + '" data-fancybox-group="group" title="' + data['title'] + '"><img src="' + data['thumb_image'] + '" alt="' + data['title'] + '" /><p>' + data['title'] + '</p></a>';
        x += '</li>';

        return x;
    };