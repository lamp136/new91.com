/*banner*/
.banner{
	width: 100%;
	max-width: 1920px;
	margin: 0 auto;
	height: 635px;
	border-top: 2px solid #2b916b;
}
.banner img{
	width:100%;
	height: 100%;
}
/*banner end*/
/*副导航*/
.nav-deali{
	width:1200px;
	height: 170px;
	margin: 0 auto;
	background: url(../../images/moon/numuto-nav_01-02.png)no-repeat;
	background-size:cover;
	position: absolute;
	left:calc(50% - 600px);
	top:510px;
}
.nav-deali a{
	position: absolute;
	top:34px;
}
.nav-deali>a:nth-child(1){
	left:11px;
}
.nav-deali>a:nth-child(2){
	left:301px;
}
.nav-deali>a:nth-child(3){
	left:597px;
}
.nav-deali>a:nth-child(4){
	left:903px;
}
/*副导航结束*/
/*content*/
.content{
	width:100%;
	height: 2800px;
	margin: 0 auto;
	padding-top: 72px;
	max-width: 1920px;
	background: url(../../images/moon/autumo-bg.jpg)no-repeat;
	background-size:100% 100%;
}
.content p{
	color:#432614;
	font-size: 15px;
	font-family: "微软雅黑";
	line-height: 30px;
}
.content>div{
	width: 1098px;
	margin: 0 auto 45px;     
}
/*内容一*/
.content .autumo-first{
	height: 230px;
	background: url(../../images/moon/main-1.jpg)no-repeat;
	background-size:cover ;
	padding: 123px 47px 30px 55px;
}
.content .autumo-first .autumo-con{
	width:1098px;
	height: 115px;
}
.content .autumo-first .autumo-con .autumo-img{
	float: left;
	width:168px;
	height: 115px;
}
.content .autumo-first .autumo-con .autumo-img img{
	width: 100%;
	height: 100%;
}
.content .autumo-first .autumo-con .autumo-fon{
	width: 914px;
	height: 115px;
	float: right;
}
.content .autumo-first .autumo-fon-first{
	font-size: 13px;
	clear: both;
	margin: 10px 0 16px;
}
.content .autumo-first .autumo-fon-second a{
	color:#4dadff;
}
/*内容一结束*/
/*内容二*/
.content .autumo-second{
	height: 697px;
	padding:103px 47px 40px 55px;
	background: url(../../images/moon/main-2.jpg)no-repeat;
	background-size:1195px auto;
}
.content .autumo-second .autumo-fon-third{
	margin-bottom: 34px;
}
/*内容二结束*/
/*内容三*/
.content .autumo-third{
	height: 620px;
	background: url(../../images/moon/mian-3.jpg)no-repeat;
	background-size:cover ;
	padding: 86px 47px 44px 55px;
}
.content .autumo-third ul li{
	padding: 20px 0 14px 0;
	height: 190px;
}
.content .autumo-third ul li .autumo-third-left{
	width: 190px;
	height: 170px;
	float: left;
}
.content .autumo-third ul li .autumo-third-right{
	width: 880px;
	height: 190px;
	float: right;
}
.content .autumo-third ul li .autumo-third-right h4{
	font-weight: bold;
	line-height: 30px;
}
.content .autumo-third ul li .autumo-third-right p a{
	color:#4dadff;
}
/*内容三结束*/
/*内容四*/
.content .autumo-fourth{
	height: 530px;
	background: url(../../images/moon/main-4.jpg)no-repeat;
	background-size:100% ;
	padding: 75px 47px 0px 55px;
}
.content .autumo-fourth ul li{
	width:253px;
	height: 90px;
	float: left;
	margin-left: 45px;
	margin-top: 30px;
	padding: 57px 20px 20px 30px;
}
.content .autumo-fourth ul li p{
	font-size: 12px;
	line-height: 22px;
	color:#5a5a5a;
	width: 100%;
	margin-top: 5px;
}
.content .autumo-fourth ul li:nth-child(1){
	background:url(../../images/moon/nutumo-list-1.jpg)no-repeat;
	background-size: 100% 100%;
}
.content .autumo-fourth ul li:nth-child(2){
	background: url(../../images/moon/nutumo-list-2.jpg)no-repeat;
	background-size: 100% 100%;
}
.content .autumo-fourth ul li:nth-child(3){
	background: url(../../images/moon/nutumo-list-3.jpg)no-repeat;
	background-size: 100% 100%;
}
.content .autumo-fourth ul li:nth-child(4){
	background: url(../../images/moon/nutumo-list-4.jpg)no-repeat;
	background-size: 100% 100%;
}
.content .autumo-fourth ul li:nth-child(5){
	background: url(../../images/moon/nutumo-list-5.jpg)no-repeat;
	background-size: 100% 100%;
}
.content .autumo-fourth ul li:nth-child(6){
	background: url(../../images/moon/nutumo-list-6.jpg)no-repeat;
	background-size: 100% 100%;
}
.content .autumo-fourth .autumo-fourth-bot{
	width:1098px;
	clear: both;
	text-align: center;
	color:#c8875f;
	font-size: 18px;
	line-height: 135px;
}
/*内容四结束*/
/*content end*/
